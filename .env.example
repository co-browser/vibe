# Vibe Browser Environment Configuration
# Copy this file to .env and fill in your values

# OpenAI Configuration
OPENAI_API_KEY=your-openai-api-key-here

# Privy Authentication (for cloud RAG)
PRIVY_APP_ID=your-privy-app-id
PRIVY_APP_SECRET=your-privy-app-secret

# Sentry Error Tracking (optional)
SENTRY_DSN=your-sentry-dsn-here
SENTRY_AUTH_TOKEN=your-sentry-auth-token

# Development Settings
NODE_ENV=development
LOG_LEVEL=info

# Chrome DevTools Protocol (development only)
DEBUG_CDP=false
CDP_PORT=9223

# RAG Server Configuration
USE_LOCAL_RAG_SERVER=true
RAG_SERVER_PORT=3001

# Gmail OAuth (optional)
GMAIL_CLIENT_ID=your-gmail-client-id
GMAIL_CLIENT_SECRET=your-gmail-client-secret

# Memory/Performance Settings
VIBE_PREVENT_SLEEP=false

# Feature Flags
ENABLE_MCP=true

# Security Settings
VIBE_ENCRYPTION_KEY=your-strong-encryption-key-here
VIBE_ENABLE_STRICT_CSP=true
VIBE_ALLOW_DEVTOOLS=false
VIBE_ENFORCE_CODE_SIGNING=true

# Content Security Policy Allowed Domains (comma-separated)
CSP_ALLOWED_DOMAINS=api.openai.com,auth.privy.io,api.privy.io,sentry.io